#include <iostream>
#include <vector>
using namespace std;
int main()
{
	vector <int>num{ 23,4,67,89,76,56 };
	cout << "The size of the vector is==> " << num.size() << endl;
	for (int i = 0; i < num.size(); i++)
	{
		cout << num[i] << endl;
	}
	num.erase(num.begin(), num.end() - 2);
	cout << "The size of the new vector is ==> " << num.size() << endl;
	for (int i = 0; i < num.size(); i++)
	{
		cout << num[i] << endl;
	}
}
